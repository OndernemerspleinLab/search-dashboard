{
	"name": "search-dashboard",
	"version": "0.1.0",
	"private": true,
	"homepage": ".",
	"dependencies": {
		"babel-polyfill": "^6.26.0",
		"gh-pages": "^1.0.0",
		"history": "^4.7.2",
		"react": "^15.6.1",
		"react-dom": "^15.6.1",
		"react-redux": "^5.0.6",
		"react-scripts": "1.0.12",
		"recompose": "^0.25.0",
		"redux": "^3.7.2",
		"redux-first-router": "^1.9.19",
		"redux-thunk": "^2.2.0",
		"serve": "^6.0.6",
		"styled-components": "^2.1.2",
		"url-polyfill": "^1.0.7",
		"whatwg-fetch": "^2.0.3"
	},
	"scripts": {
		"precommit": "lint-staged",
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test --env=jsdom",
		"test:staged":
			"cross-env CI=true react-scripts test --env=jsdom --findRelatedTests",
		"eject": "react-scripts eject",
		"lint": "eslint --ignore-path .gitignore .",
		"lint:staged": "eslint --ignore-path .gitignore",
		"format": "prettier --write src/**/*.js",
		"up": "serve -o build",
		"preserve": "yarn run build",
		"serve": "yarn run up"
	},
	"lint-staged": {
		"src/**/*.js": ["lint:staged", "prettier --write", "git add", "test:staged"]
	},
	"devDependencies": {
		"cross-env": "^5.0.5",
		"eslint": "^4.5.0",
		"eslint-plugin-import": "^2.7.0",
		"eslint-plugin-react": "^7.3.0",
		"husky": "^0.14.3",
		"lint-staged": "^4.0.4",
		"prettier": "^1.6.0"
	}
}
